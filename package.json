{
  "name": "jshint-more-stylish",
  "version": "1.0.1",
  "description": "Stylish reporter for JSHint",
  "license": "MIT",
  "author": "Kiril Vatev <vatev.1@gmail.com>",
  "contributors": [
    "Sindre Sorhus <sindresorhus@gmail.com>"
  ],
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha",
    "jshint": "jshint",
    "default-reporter": "jshint fixture.js",
    "stylish-reporter": "jshint --reporter=index.js fixture.js"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "jshint",
    "reporter",
    "formatter",
    "format",
    "lint",
    "validate",
    "stylish",
    "elegant",
    "pretty"
  ],
  "dependencies": {
    "chalk": "^1.0.0",
    "log-symbols": "^1.0.0",
    "plur": "^2.1.0",
    "string-length": "^1.0.0",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "mocha": "*",
    "jshint": "^2"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catdad/jshint-more-stylish.git"
  },
  "bugs": {
    "url": "https://github.com/catdad/jshint-more-stylish/issues"
  },
  "homepage": "https://github.com/catdad/jshint-more-stylish#readme"
